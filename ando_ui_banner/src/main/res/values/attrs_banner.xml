<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--============================Banner=========================================-->
    <!-- BaseBanner -->
    <declare-styleable name="BaseBanner">
        <!-- Banner高宽比,范围0-1,默认0.5(for LoopViewPager) , -1 按照图片比例显示-->
        <attr name="banner_scale" format="float" />

        <!-- == LoopViewPager start == -->
        <!-- 是否支持循环,默认true-->
        <attr name="banner_isLoopEnable" format="boolean" />
        <!-- 滚动延时,默认5秒-->
        <attr name="banner_delay" format="integer" />
        <!-- 滚动间隔,默认5秒-->
        <attr name="banner_period" format="integer" />
        <!-- 是否支持自动滚动,默认true-->
        <attr name="banner_isAutoScrollEnable" format="boolean" />
        <!-- == LoopViewPager end == -->
    </declare-styleable>

</resources>